{
  "name": "react-component-library",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "engines": {
    "node": ">=2.16.2",
    "npm": ">=6.14.4"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.js' --fix --ignore-pattern '*.test.js'",
    "test": "jest --coverage --config=./jest/jest.config.js",
    "start": "NODE_ENV=development styleguidist server",
    "build:": "NODE_ENV=production rm -rf dist && babel -d ./dist ./src -s && npm run build:styles",
    "build:styleguide": "NODE_ENV=production rm -rf build && yarn test && styleguidist build",
    "build:styles": "webpack --config webpack.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
