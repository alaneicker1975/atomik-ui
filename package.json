{
  "name": "@atomikui/core",
  "version": "0.0.0",
  "description": "A Comprehensive Library of Accessible React Components",
  "keywords": [
    "atomik-ui",
    "atomik",
    "ui",
    "react",
    "component library"
  ],
  "main": "./dist/index.js",
  "engines": {
    "node": ">=12.16.2",
    "npm": ">=6.14.4"
  },
  "author": "Alan Eicker <alaneicker@gmail.com> (http://www.alaneicker.com)",
  "homepage": "https://www.atomikui.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomikui/atomikui-core"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "release:major": "npm version major -m \"Upgrade to %s\" && npm run push-release",
    "release:minor": "npm version minor -m \"Upgrade to %s\" && npm run push-release",
    "release:patch": "npm version patch -m \"Upgrade to %s\" && npm run push-release",
    "push-release": "git push origin master",
    "lint:js": "eslint 'src/**/*.js' --fix --ignore-pattern '*.test.js'",
    "lint:sass": "stylelint ./src/components/**/*.scss ./sass-utilities/**/*.scss --config .stylelintrc.json --fix",
    "test": "jest --coverage --silent --config=./jest/jest.config.js",
    "start": "NODE_ENV=development styleguidist server",
    "splash": "node-sass --watch ./splash/ --output ./splash/ --recursive false",
    "build:atomik": "NODE_ENV=production babel --ignore src/components/**/*.test.js -d ./dist ./src -s && cp package.json dist/package.json",
    "build:styles": "cross-env webpack -p --config webpack.config.js",
    "build:styleguide": "NODE_ENV=production yarn styleguidist build && cp -R splash build && cp src/CNAME build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "card-validator": "^6.2.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "country-flag-icons": "^1.2.3",
    "d3-ease": "^1.0.6",
    "downshift": "^5.0.6",
    "focus-trap": "^5.1.0",
    "lodash.sample": "^4.2.1",
    "lodash.shuffle": "^4.2.0",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react-calendar": "^3.0.1",
    "react-flexbox-grid": "^2.1.2",
    "react-masked-field": "^2.0.3",
    "react-merge-refs": "^1.1.0",
    "react-spring": "^8.0.27",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@atomikui/core-sass": "^0.1.0",
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@svgr/webpack": "^5.4.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "formik": "^2.1.4",
    "jest": "^26.0.1",
    "jest-html-reporters": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-axe": "^3.5.3",
    "react-dom": "^16.13.1",
    "react-styleguidist": "9.2.0",
    "react-table": "^7.0.4",
    "sass-loader": "7.3.0",
    "sinon": "^9.0.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.1",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-fix-style-only-entries": "^0.4.0",
    "whatwg-fetch": "^3.0.0",
    "yup": "^0.28.3"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.1"
  }
}
